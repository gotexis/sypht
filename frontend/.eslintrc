{
  "extends": [
    "react-app",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/destructuring-assignment": [
      0
    ],
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ],
    "jsx-a11y/anchor-is-valid": "off",
    "react/button-has-type": "off"
  }
}

